{
    "name": "cica-review-case-documents",
    "version": "0.0.1",
    "engines": {
        "npm": ">=10.8.2",
        "node": ">=22.8.0"
    },
    "private": true,
    "type": "module",
    "license": "MIT",
    "scripts": {
        "start:dev": "nodemon -L --inspect=0.0.0.0:9229 -e .css,.scss,.js,.json,.njk --ignore public/ --ignore src/js/scripts.babel.generated.js npm start",
        "start": "node --env-file=.env ./bin/www",
        "sass": "sass --load-path=. --style=compressed --no-source-map src/sass/all.scss public/stylesheets/all.css",
        "build:dev": "npm run babel && npm run webpack:dev",
        "build": "npm run babel && npm run webpack",
        "webpack:dev": "webpack --config webpack.config.js",
        "webpack": "webpack --config webpack.config.prod.js",
        "babel": "babel src/js/scripts.js -o src/js/scripts.babel.generated.js",
        "test": "node --env-file=.env.test --test"
    },
    "dependencies": {
        "@ministryofjustice/frontend": "^5.1.5",
        "@opensearch-project/opensearch": "^3.5.1",
        "cookie-parser": "^1.4.7",
        "csrf-csrf": "^4.0.3",
        "express": "^5.1.0",
        "express-openapi-validator": "^5.6.0",
        "express-session": "^1.18.2",
        "got": "^14.4.9",
        "helmet": "^8.1.0",
        "lodash.merge": "^4.6.2",
        "nanoid": "^5.1.5",
        "nunjucks": "^3.2.4",
        "pino-http": "^11.0.0",
        "pino-pretty": "^13.1.2",
        "supertest": "^7.1.4",
        "verror": "^1.10.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.28.3",
        "@babel/core": "^7.28.4",
        "@babel/plugin-transform-runtime": "^7.28.3",
        "@babel/preset-env": "^7.28.3",
        "babel-loader": "^10.0.0",
        "core-js": "^3.45.1",
        "nodemon": "^3.1.10",
        "sass": "^1.92.1",
        "webpack-cli": "^6.0.1"
    }
}
