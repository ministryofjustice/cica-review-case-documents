{
    "name": "cica-review-case-documents",
    "version": "0.0.1",
    "engines": {
        "npm": ">=10.8.2",
        "node": ">=22.8.0"
    },
    "private": true,
    "type": "module",
    "license": "MIT",
    "scripts": {
        "start:dev": "nodemon -L --inspect=0.0.0.0:9229 -e .css,.scss,.js,.json,.njk --ignore public/ --ignore src/js/scripts.babel.generated.js npm start",
        "start": "node ./bin/www",
        "prestart:dev": "npm run openapi:build",
        "sass": "sass --load-path=. --style=compressed --no-source-map src/sass/all.scss public/stylesheets/all.css",
        "build:dev": "npm run babel && npm run webpack:dev",
        "build": "npm run babel && npm run webpack",
        "webpack:dev": "webpack --config webpack.config.js",
        "webpack": "webpack --config webpack.config.prod.js",
        "babel": "babel src/js/scripts.js -o src/js/scripts.babel.generated.js",
        "test": "c8 node --env-file=.env.test --test",
        "coverage:html": "c8 report --reporter=html",
        "lint": "biome check",
        "format": "biome format",
        "jsdoc:check": "eslint .",
        "openapi:build": "node ./api/openapi/buildOpenApiSpec.js"
    },
    "dependencies": {
        "@apidevtools/json-schema-ref-parser": "^15.1.3",
        "@ministryofjustice/frontend": "^7.1.1",
        "@opensearch-project/opensearch": "^3.5.1",
        "ajv": "^8.17.1",
        "ajv-errors": "^3.0.0",
        "cookie-parser": "^1.4.7",
        "csrf-csrf": "^4.0.3",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-openapi-validator": "^5.6.0",
        "express-rate-limit": "^8.2.1",
        "express-session": "^1.18.2",
        "got": "^14.6.5",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.3",
        "lodash.merge": "^4.6.2",
        "nanoid": "^5.1.6",
        "nunjucks": "^3.2.4",
        "pino-http": "^11.0.0",
        "pino-pretty": "^13.1.3",
        "supertest": "^7.1.4",
        "uuid": "^13.0.0",
        "verror": "^1.10.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.28.3",
        "@babel/core": "^7.28.5",
        "@babel/plugin-transform-runtime": "^7.28.5",
        "@babel/preset-env": "^7.28.5",
        "@biomejs/biome": "2.3.8",
        "babel-loader": "^10.0.0",
        "c8": "^10.1.3",
        "core-js": "^3.47.0",
        "eslint-plugin-jsdoc": "^61.4.1",
        "husky": "^9.1.7",
        "nodemon": "^3.1.11",
        "sass": "^1.94.2",
        "webpack-cli": "^6.0.1"
    }
}
